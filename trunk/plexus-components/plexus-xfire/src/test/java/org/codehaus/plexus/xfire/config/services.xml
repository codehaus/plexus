<xfire>
  
  <services>
    <service>
      <name>EchoIntf</name>
      <namespace>urn:Echo</namespace>
      <soapVersion>1.1</soapVersion>
      <encoding>wrapped</encoding>
      <use>literal</use>
      <serviceClass>org.codehaus.plexus.xfire.config.EchoIntf</serviceClass>
      <implementationClass>org.codehaus.plexus.xfire.config.Echo</implementationClass>
    </service>

    <service>
      <name>Echo</name>
      <namespace>urn:Echo</namespace>
      <soapVersion>1.1</soapVersion>
      <encoding>wrapped</encoding>
      <use>literal</use>
      <serviceClass>org.codehaus.plexus.xfire.config.Echo</serviceClass>
      
      <inHandlers>
        <handler>test</handler>
        <handler>org.codehaus.plexus.xfire.config.TestHandler</handler>
      </inHandlers>
      
      <outHandlers>
        <handler>test</handler>
      </outHandlers>
    </service>

    <service>
      <name>EchoXMLBeans</name>
      <namespace>urn:EchoXMLBeans</namespace>
      <soapVersion>1.1</soapVersion>
      <encoding>wrapped</encoding>
      <use>literal</use>
      <serviceClass>org.codehaus.plexus.xfire.config.Echo</serviceClass>
      <serviceBuilder>org.codehaus.plexus.xmlbeans.XMLBeansServiceBuilder</serviceBuilder>
    </service>

  </services>
  
</xfire>