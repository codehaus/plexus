<plexus>
  <load-on-start>
    <component>
      <role>org.codehaus.plexus.naming.Naming</role>
      <role-hint>environments</role-hint>
    </component> 
  </load-on-start>  
  <components>
    <component>
      <role>org.codehaus.plexus.naming.Naming</role>
      <role-hint>environments</role-hint>
      <implementation>org.codehaus.plexus.naming.DefaultNaming</implementation>
      <configuration>
        <environments>
          <environment>
            <name>two</name>
            <type>java.lang.Integer</type>
            <value>2</value>
          </environment>
          <environment>
            <name>string</name>
            <type>java.lang.String</type>
            <value>foo</value>
          </environment> 
          <environment>
            <name>boolean</name>
            <type>java.lang.Boolean</type>
            <value>true</value>
          </environment>
          <environment>
            <name>subOne/firstEntry</name>
            <type>java.lang.String</type>
            <value>entryOne</value>
          </environment>
          <environment>
            <name>subOne/secondEntry</name>
            <type>java.lang.String</type>
            <value>entryTwo</value>
          </environment>									                         
        </environments>
      </configuration>
    </component> 
	<component>
	  <role>org.codehaus.plexus.registry.Registry</role>	
	  <role-hint>naming</role-hint>
	  <implementation>org.codehaus.plexus.registry.naming.NamingRegistry</implementation>
	  <configuration>
	  </configuration>
	</component>  
  </components>
</plexus>