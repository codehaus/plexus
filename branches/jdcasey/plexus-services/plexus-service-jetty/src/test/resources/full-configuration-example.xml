<configuration>
  <webapps>
    <webapp>
      <file>${plexus.home}/lib/continuum-web-1.0-alpha-3-SNAPSHOT.jar</file>
      <context>/continuum</context>
      <extraction-path>${plexus.home}/webapp</extraction-path>
      <virtual-host>www.foo.com</virtual-host>
      <listeners>
        <http-listener>
          <host></host>
          <port>123</port>
        </http-listener>
        <proxy-http-listener>
          <port>8090</port>
          <proxy-host>localhost</proxy-host>
          <proxy-port>80</proxy-port>
        </proxy-http-listener>
      </listeners>
    </webapp>
  </webapps>
</configuration>
