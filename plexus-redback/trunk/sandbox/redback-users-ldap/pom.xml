    <artifactId>redback-users</artifactId>
    <version>1.0-alpha-3-SNAPSHOT</version>
  </parent>
  
  <artifactId>redback-users-ldap</artifactId>
  
  <name>redback-users-ldap</name>
  
  <dependencies>
